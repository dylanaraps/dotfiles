set dsp.oss.device=
set follow=true
set format_current=
set format_playlist= %-21%a %3n %t%= %y %d %{?X!=0?%3X ?    }
set format_playlist_va= %-21%A %3n %t %= %y %d %{?X!=0?%3X ?    }
set format_statusline= %{status} %{artist} - %{title} | %{?show_playback_position?%{position} %{?duration?/ %{duration} }?%{?duration?%{duration} }} %{?show_current_bitrate & bitrate>=0? %{bitrate} kbps }%=
set format_title=%a - %t
set format_trackwin=%3n. %t%= %y %d
